{
  "name": "vite-pwa-react-router",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.7.0",
  "description": "Zero-config PWA for React Router",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/vite-pwa/react-router#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vite-pwa/react-router.git"
  },
  "bugs": "https://github.com/vite-pwa/react-router/issues",
  "keywords": [
    "react router",
    "react router preset",
    "workbox",
    "pwa",
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./components":  "./dist/components/index.mjs",
    "./react-router-sw": {
      "types": "./react-router-sw.d.ts"
    },
    "./sw": "./dist/sw/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "typesVersions": {
    "*": {
      "components": [
        "dist/components/index.d.mts"
      ],
      "sw": [
        "dist/sw/index.d.mts"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@react-router/dev": ">=7",
    "@vite-pwa/assets-generator": "^1.0.0",
    "vite-plugin-pwa": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@vite-pwa/assets-generator": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^0.21.12",
    "@react-router/dev": "^7.4.1",
    "@types/debug": "^4.1.8",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "bumpp": "^9.2.0",
    "eslint": "^9.23.0",
    "esno": "^4.0.0",
    "https-localhost": "^4.7.1",
    "react-router": "^7.4.1",
    "typescript": "^5.4.3",
    "unbuild": "^3.5.0",
    "vite": "^5.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
